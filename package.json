  "scripts": {
    "build": "env-cmd -f .env webpack --config config/webpack.prod.js",
    "start": "env-cmd -f envs/.env.default env-cmd -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:stage-api": "env-cmd -f envs/.env.default env-cmd -f envs/.env.stage-api env-cmd -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:preprod-api": "env-cmd -f envs/.env.default env-cmd -f envs/.env.preprod-api env-cmd -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:local-user-management-module": "env-cmd -f envs/.env.default env-cmd -f envs/.env.local-user-management-module env-cmd -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:local-billing-module": "env-cmd -f envs/.env.default env-cmd -f envs/.env.local-billing-module env-cmd -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:local-all-modules": "env-cmd -f envs/.env.default env-cmd -f envs/.env.local-modules -f .env --silent webpack serve --config config/webpack.dev.js",
    "start:circular-dependency-check": "env-cmd -f envs/.env.default env-cmd -f .env env-cmd -f envs/.env.circular-dependency-check --silent webpack serve --config config/webpack.dev.js",
    "test": "node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --logHeapUsage --silent --verbose --coverage --changedFilesWithAncestor",
    "test:all": "node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --logHeapUsage --silent --verbose --coverage",
    "test:ci": "node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --logHeapUsage --silent --verbose --coverage",
    "lint": "eslint src/**/*.ts[x] --fix --max-warnings=0",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json,d.ts}' --config ./.prettierrc",
    "prepare": "husky install",
    "generate:types": "tsc -p ./tsconfig.json --noEmit false --declaration --emitDeclarationOnly",
    "dependency-check": "dependency-check ./ --project rims2 --scan ./ --out ./dependency-check-reports --disableNodeAudit --failOnCVSS 8 --exclude \"**/.idea/**\""
  },
